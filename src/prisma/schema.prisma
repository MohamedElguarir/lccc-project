datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id         String   @id @default(cuid())
  first_name String
  last_name  String
  username   String   @unique
  email      String   @unique
  avatar_url String?
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}
